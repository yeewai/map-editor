{
  "name": "map-editor-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^5.0.14",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
