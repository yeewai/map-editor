{
  "name": "map-editor-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "css-loader": "^0.28.9",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "postcss-loader": "^2.0.10",
    "prettier-eslint": "^8.8.1",
    "react": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "es-dev-watch": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack-dev.js --port 3000 -w",
    "es-prod-build": "cross-env NODE_ENV=production webpack --config scripts/webpack-prod.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm run watch-css & npm run es-dev-watch",
    "build": "npm run build-css && npm run es-prod-build",
    "start-ts": "npm run watch-css & react-scripts-ts start",
    "build-ts": "npm run watch-css & react-scripts-ts build",
    "test": "NODE_PATH=src/ react-scripts-ts test --env=jsdom",
    "eject": ""
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.7",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-typescript": "^1.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-typescript": "^0.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.6.2",
    "webpack-dev-server": "^2.11.1"
  }
}
